+void
+computeGraph (void* argPtr)
+{
+    TM_THREAD_ENTER();
+
+    graph*    GPtr       = ((computeGraph_arg_t*)argPtr)->GPtr;
+    graphSDG* SDGdataPtr = ((computeGraph_arg_t*)argPtr)->SDGdataPtr;
+
+#if ORIGINAL
