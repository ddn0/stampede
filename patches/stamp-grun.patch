-                                                  parentQueryVectorPtr);
-
-            queries[0].value = 0;
-            queries[1].value = 1;
-            parentQuery.value = ((vv < v) ? 0 : 1);
-            newLocalLogLikelihood +=
-                computeSpecificLocalLogLikelihood(adtreePtr,
-                                                  queryVectorPtr,
-                                                  parentQueryVectorPtr);
-
-            queries[0].value = 1;
-            queries[1].value = 0;
-            parentQuery.value = ((vv < v) ? 1 : 0);
