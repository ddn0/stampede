-/* =============================================================================
- * createPartition
- * =============================================================================
- */
-static void
-createPartition (long min, long max, long id, long n,
-                 long* startPtr, long* stopPtr)
-{
-    long range = max - min;
-    long chunk = MAX(1, ((range + n/2) / n)); /* rounded */
-    long start = min + chunk * id;
-    long stop;
-    if (id == (n-1)) {
-        stop = max;
-    } else {
-        stop = MIN(max, (start + chunk));
-    }
-
-    *startPtr = start;
-    *stopPtr = stop;
-}
