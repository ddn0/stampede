-/* =============================================================================
- * hashString
- * -- uses sdbm hash function
- * =============================================================================
- */
-static ulong_t
-hashString (char* str)
-{
-    ulong_t hash = 0;
-    long c;
-
-    /* Note: Do not change this hashing scheme */
-    while ((c = *str++) != '\0') {
-        hash = c + (hash << 6) + (hash << 16) - hash;
-    }
-
-    return (ulong_t)hash;
-}
-
-
-/* =============================================================================
- * hashSegment
- * -- For hashtable
- * =============================================================================
- */
