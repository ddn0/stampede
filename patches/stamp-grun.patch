+    ULONGINT_T numEdgesPlaced = SDGdataPtr->numEdgesPlaced;
+
+    /*
+     * First determine the number of vertices by scanning the tuple
+     * startVertex list
+     */
+
+    long i;
+    long i_start;
+    long i_stop;
+#if ORIGINAL
+    createPartition(0, numEdgesPlaced, myId, numThread, &i_start, &i_stop);
+
+    for (i = i_start; i < i_stop; i++) {
